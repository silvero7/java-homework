
public class Name 
{
	private String first;
	private String middle;
	private String last;
	
	// costructor methhods
	public Name(String f, String m, String l)
	{ 
		first = f;
		middle = m;
		last = l;
	}	
	public Name(String f, String l)
	{ 
		first = f;
		middle = "";
		last = l;
	}
	public Name(String l)
	{
		first = "";
		middle = "";
		last = l;
	}
	public Name()
	{
		first = "";
		middle = "";
		last = "";
	}
}
